/* Calcula maximo e minimo */
int x;
x= 3;
int max(int a, int b)
{
    if (a > b)
        return a;
    else
        return b;
}

int min(int a, int b)
{
    if (a < b)
        return a;
    else
        return b;
}

void main(void)
{
    int x;
    int y;
    
    x = 3;
    y = input();
    
    output(max(x, y));
    output(min(x, y));
}