/* Exemplo 7: Estruturas de controle */
int x;
int max(int a, int b) {
    int result;
    if (a > b)
        result = a;
    else
        result = b;
    return result;
}

void main(void) {
    int x;
    int y;
    int maior;
    
    x = 10;
    y = 20;
    maior = max(x,y);
    output (maior);
}

